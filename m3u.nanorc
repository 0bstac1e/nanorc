syntax "m3u" "\.(m3u|m3u8)$"

# Header text
color brightcyan "^#EXTINF.*,[^,]*$"

# Header property values
color brightyellow "^#[^:]*:[^ ,]*"
color brightyellow "=[^ ,]*"
color brightyellow "=[\"][^\"]*[\"]"

# Header property keys
color brightgreen "[a-zA-Z-]*="

# Headers
color brightmagenta start="#EXTM3U.*" end="$"
color brightred "^#EXT[-A-Z]*:"

# Separators
color normal "[,=]"

# URLs
color normal "^[^#].*"
